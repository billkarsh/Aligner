#!/bin/sh

# Purpose:
# For each layer in range, gather points, cd to layer's montage dir,
# run lsq there.
#
# > ./submon.sht <zmin> [zmax]


if (($# == 1))
then
	last=$1
else
	last=$2
fi

for lyr in $(seq $1 $last)
do
	echo $lyr
	if [ -d "$lyr" ]
	then
		QSUB_1NODE.sht "mon-$lyr" "" 8 "./montage1.sht $lyr"
	fi
done

